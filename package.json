{
  "name": "press-ready",
  "description": "Make your PDFs compliant with press-ready PDF/X-1a.",
  "version": "4.0.1",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "scripts": {
    "build": "tsc && shx chmod +x lib/cli.js",
    "clean": "shx rm *.pdf",
    "dev": "tsc -w",
    "release": "scripts/release.sh",
    "test": "yarn build && node ./lib/cli.js build -i test/fixture/chrome.pdf"
  },
  "dependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/mustache": "^4.0.1",
    "@types/shelljs": "^0.8.6",
    "@types/uuid": "^7.0.0",
    "@types/yargs": "^15.0.4",
    "chalk": "^4.0.0",
    "cli-table": "^0.3.1",
    "debug": "^4.1.1",
    "execa": "^4.0.0",
    "mustache": "^4.0.0",
    "shelljs": "^0.8.3",
    "uuid": "^7.0.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "eslint": "^6.2.2",
    "husky": "^4.0.7",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.0",
    "shx": "^0.3.2",
    "typescript": "^3.8.3"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib",
    "assets"
  ],
  "repository": "https://github.com/vibranthq/press-ready.git",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test"
    }
  },
  "license": "Apache-2.0",
  "bin": "lib/cli.js",
  "bugs": "https://github.com/vibranthq/press-ready/issues",
  "homepage": "https://github.com/vibranthq/press-ready",
  "keywords": [
    "press-ready",
    "printing"
  ]
}
